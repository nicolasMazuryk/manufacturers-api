{
  "presets": [
    "react-app"
  ],
  "plugins": [
    [
      "react-css-modules",
      {
        "filetypes": {
          ".scss": {
            "syntax": "postcss-scss"
          }
        },
        "generateScopedName": "[name]__[local]",
        "handleMissingStyleName": "warn"
      }
    ]
  ]
}